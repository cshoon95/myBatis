<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTO Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.myBatis.myService.dao.MemberDao">
    <select id="selectList" parameterType="com.myBatis.myService.model.Member" resultType="com.myBatis.myService.model.Member">
        SELECT deptno
             , dname
             , loc_code AS loc
        FROM DEPT
    </select>
</mapper>